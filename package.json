{
  "name": "exynize-rest",
  "version": "0.4.2",
  "description": "Exynize REST server, part of Exynize platform",
  "main": "index.js",
  "scripts": {
    "start": "npm run start-db && npm run start-rabbit && npm run server",
    "server": "nodemon --ignore src/static index.js",
    "start-db": "docker run -d -p 28015:28015 -p 8080:8080 --name rethinkpipes rethinkdb",
    "stop-db": "docker stop rethinkpipes",
    "start-rabbit": "docker run -d -p 5672:5672 -p 8081:15672 --name exynize-rabbit rabbitmq:management",
    "stop-rabbit": "docker stop exynize-rabbit",
    "build-docker": "docker build -t exynize-rest .",
    "start-docker": "docker run -it --rm --link rethinkpipes:rdb -e EXYNIZE_DB_HOST=rdb --name exynize-rest exynize-rest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Exynize/exynize-rest.git"
  },
  "author": "Tim Ermilov <yamalight@gmail.com> (http://codezen.net)",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/Exynize/exynize-rest",
  "dependencies": {
    "amqplib": "^0.4.0",
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-plugin-typecheck": "^3.4.6",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "bluebird": "^3.0.6",
    "body-parser": "^1.14.1",
    "bootstrap": "^3.3.6",
    "bootstrap-material-design": "^0.3.0",
    "cors": "^2.7.1",
    "css-loader": "^0.23.0",
    "express": "^4.13.3",
    "express-ws": "^1.0.0-rc.2",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^5.4.1",
    "lodash": "^3.10.1",
    "method-override": "^2.3.5",
    "node-uuid": "^1.4.7",
    "nodemailer": "^1.10.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "rethinkdb": "^2.2.0",
    "rx": "^4.0.6",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.6",
    "winston": "^2.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.8.1"
  }
}
